// Net burglar example
.name "ncp0"
.type LPN
.description "Testing not concurrency preserving net 0"
.options
winningCondition="SAFETY"

.places
env1[env="true"] 
env2[env="true"]
sys1
sys2
sys3
sys4
sys5
sys6[bad="true"]

.transitions
t1
t2
t3
t4
t5

.flows
// env separated
t1: {env1} -> {env2}
// system
t2: {sys1} -> {sys2}
t3: {sys1} -> {sys3,sys4}
t4: {sys3} -> {sys5}
t5: {sys4} -> {sys6}

.initial_marking {env1, sys1}
